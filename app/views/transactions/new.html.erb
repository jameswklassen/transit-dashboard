<h1>New Transaction</h1>



<%= form_with model: @transaction  do |f| %>

  <%= f.label :date %>
  <%= f.text_field :date %>
  <br>
  <%= f.label :transaction_number %>
  <%= f.text_field :transaction_number %>
  <br>
  <%= f.label :transit_agency %>
  <%= f.text_field :transit_agency %>
  <br>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :transaction_type %>
  <%= f.text_field :transaction_type %>
  <br>
  <%= f.label :service_class %>
  <%= f.text_field :service_class %>
  <br>
  <%= f.label :discount %>
  <%= f.text_field :discount %>
  <br>
  <%= f.label :amount %>
  <%= f.text_field :amount %>
  <br>
  <%= f.label :balance %>
  <%= f.text_field :balance %>
  <br>
  <%= f.submit "Create" %>
<% end %>